.ant-empty {
  text-align: center;
}
.ant-empty-normal .ant-empty-image-element {
  width: 5.1rem;
  height: 2.82rem;
}
.ant-empty-normal .ant-empty-text-main {
  color: var(--empty-text-color, #333333);
  font-size: 0.36rem;
  margin-top: 0.24rem;
}
.ant-empty-normal .ant-empty-text-sub {
  margin-top: 0.12rem;
  color: var(--empty-asisst-text-color, #999999);
}
.ant-empty-normal .ant-empty-button {
  padding-top: 0.8rem;
}
.ant-empty-small .ant-empty-image-element {
  width: 4.16rem;
  height: 2.3rem;
}
.ant-empty-small .ant-empty-text-main {
  color: var(--empty-asisst-text-color, #999999);
  font-size: 0.32rem;
  margin-top: 0.24rem;
}
.ant-empty-small .ant-empty-text-sub {
  color: #cccccc;
  margin-top: 0.08rem;
}
.ant-empty-small .ant-empty-button {
  padding-top: 0.32rem;
}
.ant-empty-image {
  display: flex;
  justify-content: center;
}
.ant-empty-text {
  margin-top: 0.24rem;
  line-height: 0.5rem;
}
.ant-empty-text-main {
  color: #333333;
  font-size: 0.36rem;
}
.ant-empty-text-sub {
  font-size: 0.24rem;
  margin-top: 0.32rem;
  line-height: 0.37rem;
}
.ant-empty-button-layout-horizontal {
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 0.24rem;
}
.ant-empty-button-layout-vertical {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  gap: 0.32rem;
}
.ant-empty-button-layout-vertical .ant-button {
  min-width: 3.68rem;
}
.ant-empty-extra {
  padding-top: 0.32rem;
}
