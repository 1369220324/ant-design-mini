.ant-loading-small {
  width: 0.35rem;
  height: 0.35rem;
}
.ant-loading-medium {
  width: 0.7rem;
  height: 0.7rem;
}
.ant-loading-large {
  width: 1.05rem;
  height: 1.05rem;
}
.ant-loading-x-large {
  width: 1.4rem;
  height: 1.4rem;
}
.ant-loading-spin {
  display: inline-flex;
  justify-content: center;
  align-items: center;
}
.ant-loading-spin-icon {
  width: 100%;
  height: 100%;
  background-position: center;
  background-repeat: no-repeat;
}
.ant-loading-mini {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  min-width: 0.66rem;
  min-height: 0.66rem;
}
.ant-loading-mini-item {
  flex: 0 0 0.08rem;
  min-width: 0.08rem;
  min-height: 0.08rem;
  max-width: 0.08rem;
  max-height: 0.08rem;
  overflow: hidden;
  margin-right: 0.16rem;
  font-size: 0;
  border-radius: 0.04rem / 2;
  background-color: #999999;
  animation: ant-loading-animation 1s 0s infinite linear;
}
.ant-loading-mini-item__1 {
  animation-delay: 0s;
}
.ant-loading-mini-item__2 {
  animation-delay: 150ms;
}
.ant-loading-mini-item__3 {
  margin-right: 0;
  animation-delay: 300ms;
}
@keyframes ant-loading-animation {
  0% {
    transform: translate3d(0, 0, 0);
  }
  12% {
    transform: translate3d(0, -150%, 0);
  }
  40% {
    transform: translate3d(0, 110%, 0);
  }
  55% {
    transform: translate3d(0, -10%, 0);
  }
  60% {
    transform: translate3d(0, 0, 0);
  }
  100% {
    transform: translate3d(0, 0, 0);
  }
}
