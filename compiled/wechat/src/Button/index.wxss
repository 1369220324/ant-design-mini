.ant-button {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-self: center;
  align-items: center;
  height: auto;
  line-height: normal;
  padding: var(--size-font3, 12px);
  border-radius: 50vh;
  border: 0 none;
  box-sizing: border-box;
  font-size: var(--size-font8, 18px);
  z-index: 2;
}
.ant-button-large {
  font-size: var(--size-font8, 18px);
  padding: var(--size-font3, 12px);
}
.ant-button-medium {
  font-size: var(--size-font7, 17px);
  padding: var(--size-font2, 8px) var(--size-font3, 12px);
}
.ant-button-small {
  font-size: var(--size-font5, 15px);
  padding: var(--size-font1, 4px) var(--size-font3, 12px);
}
.ant-button-primary {
  color: var(--color-white, #ffffff);
  background-color: var(--color-brand1, #1677ff);
  box-shadow: inset 0 0 0 1px var(--color-brand1, #1677ff);
}
.ant-button-default {
  color: var(--color-brand1, #1677ff);
  background-color: var(--color-white-change, #ffffff);
  box-shadow: inset 0 0 0 1px var(--color-brand2, #1677ff);
}
.ant-button-capsule {
  color: var(--color-white, #ffffff);
  background-color: #ffffff;
  box-shadow: none;
}
.ant-button-primary-aide {
  background-color: var(--color-wathet, #e7f1ff);
  box-shadow: inset 0 0 0 1px var(--color-wathet, #e7f1ff);
  color: var(--color-brand1, #1677ff);
}
.ant-button-default-aide {
  color: var(--color-text-primary, #333333);
  background-color: var(--color-white-change, #ffffff);
  box-shadow: inset 0 0 0 1px var(--color-border-greycard, #e5e5e5);
}
.ant-button-text {
  color: #1677ff;
  background-color: transparent;
  box-shadow: none;
}
.ant-button-primary-danger {
  color: var(--color-white, #ffffff);
  background-color: var(--color-red, #ff3141);
  box-shadow: inset 0 0 0 1px var(--color-red, #ff3141);
}
.ant-button-default-danger {
  color: var(--color-red, #ff3141);
  background-color: var(--color-white, #ffffff);
  box-shadow: inset 0 0 0 1px var(--color-red, #ff3141);
}
.ant-button-text-danger {
  color: var(--color-red, #ff3141);
  background-color: transparent;
  box-shadow: none;
}
.ant-button-text-active {
  background-color: rgba(255, 255, 255, 0.92);
}
.ant-button-active {
  filter: brightness(0.92);
}
.ant-button-disabled {
  opacity: var(--opacity-disable, 0.4);
}
.ant-button-wrap {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.ant-button-content-text-margin {
  margin-left: 8px;
}
.ant-button-content-text:empty {
  margin-left: 0;
  width: 0;
  opacity: 0;
}
.ant-button-content-text:empty::after {
  content: '\00a0';
}
.ant-button-content-subtext {
  font-size: var(--size-font2, 12px);
  opacity: 0.6;
}
.ant-button-content-loading-container {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 33px;
  height: 16px;
  margin-left: 8px;
}
.ant-button-content-loading {
  position: relative;
}
.ant-button-inline {
  display: inline-block;
  border-radius: 50vh;
}
.ant-button-inline .ant-button-content-loading-container {
  width: 22px;
  height: 22px;
}
.ant-button-inline .ant-button-content-loading {
  transform: scale(0.5);
}
.ant-button::after {
  border-width: 0;
}
