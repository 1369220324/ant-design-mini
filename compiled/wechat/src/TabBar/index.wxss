.ant-tab-bar-wrap {
  display: flex;
  align-items: center;
  background-color: var(--tabbar-bg, #ffffff);
}
.ant-tab-bar-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 0.08rem 0;
  color: var(--tabbar-item-color, #666666);
}
.ant-tab-bar-item-active {
  color: var(--tabbar-active-color, #1677ff);
}
.ant-tab-bar-icon {
  font-size: 0.44rem;
}
.ant-tab-bar-large-icon {
  font-size: 0.72rem;
}
.ant-tab-bar-image {
  font-size: 0.44rem;
  max-width: 0.44rem;
  max-height: 0.44rem;
}
.ant-tab-bar-text {
  font-size: 0.2rem;
}
