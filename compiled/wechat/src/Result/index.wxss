.ant-result {
  position: relative;
  text-align: center;
  line-height: normal;
  box-sizing: border-box;
}
.ant-result-icon {
  font-size: 55px;
}
.ant-result-main {
  padding: 35px 12px;
  margin-bottom: 20px;
  line-height: 1.4;
  background-color: #ffffff;
}
.ant-result-image {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 64px;
  height: 64px;
  margin: 0 auto;
  overflow: hidden;
  text-align: center;
  line-height: 64px;
}
.ant-result-image-success {
  color: #1677ff;
}
.ant-result-image-error {
  color: #ff3b30;
}
.ant-result-image-info {
  color: #1677ff;
}
.ant-result-image-warning {
  color: #ff8f1f;
}
.ant-result-image-wait {
  color: #00b578;
}
.ant-result-title {
  padding-top: 20px;
  font-size: 20px;
  color: #000000;
  font-weight: 500;
}
.ant-result-title:empty {
  display: none;
}
.ant-result-message {
  padding-top: 8px;
  font-size: 16px;
  color: #333333;
}
.ant-result-message:empty {
  display: none;
}
