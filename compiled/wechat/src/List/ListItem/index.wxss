.ant-list-item {
  align-items: center;
  background-color: #ffffff;
  color: #333333;
  line-height: 1.4;
}
.ant-list-item-hover {
  background-color: #eeeeee;
}
.ant-list-item-line {
  flex: 1;
  display: flex;
  padding: 0 12px;
  align-items: center;
  position: relative;
}
.ant-list-item-line-divider::after {
  content: '';
  position: absolute;
  background-color: #eeeeee;
  display: block;
  top: auto;
  right: 0;
  bottom: 0;
  left: 0;
  height: 1px;
  transform: scaleY(0.5);
}
.ant-list-item-line-disabled {
  opacity: 0.4;
}
.ant-list-item-image {
  border-radius: 50vh;
}
.ant-list-item-image-large-icon {
  font-size: 40px;
}
.ant-list-item-image-large-image {
  width: 40px;
  height: 40px;
}
.ant-list-item-image-normal-icon {
  font-size: 28px;
}
.ant-list-item-image-normal-image {
  width: 28px;
  height: 28px;
}
.ant-list-item-image-container {
  margin-right: 12px;
}
.ant-list-item-image-container:empty {
  display: none;
}
.ant-list-item-content-container {
  display: flex;
  flex: 1;
  flex-direction: column;
  padding: 12px 0;
}
.ant-list-item-content-container-small-text {
  font-size: 12px;
  line-height: 16.5px;
}
.ant-list-item-content-container-normal-text {
  font-size: 14px;
  line-height: 20px;
}
.ant-list-item-content-main {
  font-size: 16px;
  line-height: 22.5px;
}
.ant-list-item-content-title-container {
  color: #999999;
}
.ant-list-item-content-brief-container {
  color: #999999;
}
.ant-list-item-extra {
  font-size: 16px;
  line-height: 22.5px;
  display: flex;
  flex-direction: row-reverse;
}
.ant-list-item-extra:empty {
  display: none;
}
.ant-list-item-extra-container {
  display: flex;
  flex-direction: column;
}
.ant-list-item-extra-brief {
  display: flex;
  flex-direction: row-reverse;
  font-size: 14px;
  line-height: 20px;
  color: #999999;
}
.ant-list-item-arrow {
  margin-left: 4px;
  color: #cccccc;
  display: flex;
  align-items: center;
}
.ant-list-item-arrow .ant-icon {
  font-size: 18px;
}
.ant-list-item-arrow-large-margin {
  margin-left: 12px;
}
.ant-list-item .line {
  height: 0.5px;
  background-color: #eeeeee;
  width: calc(100% - 24 * 0.5px);
  position: absolute;
  right: 0;
}
