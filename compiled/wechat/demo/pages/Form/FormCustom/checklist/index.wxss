/* 给css变量的默认底色，用来兜底使用 */
.input {
  border: 1px solid #e5e5e5;
  padding: 4px;
  min-height: 25px;
  border-radius: 4px;
  display: flex;
  align-items: center;
}
.choose {
  color: #cccccc;
}
.input-items {
  flex: 1;
  display: flex;
}
.input-item {
  text-align: center;
  padding: 0 8px 0 8px;
  margin-right: 8px;
  background-color: #f5f5f5;
}
