<view
  a:if="{{ loading && rows > 0 }}"
  class="ant-skeleton-paragraph {{ animate ? 'ant-skeleton-paragraph-animate' : '' }} {{ className || '' }}"
  style="{{ style }}">
  <block
    a:for="{{ Array.from({
  length: rows,
}) }}"
    a:for-index="index"
    a:for-item="_"
    a:key="{{ index }}">
    <view class="ant-skeleton-paragraph-row" />
  </block>
</view>
<slot a:else />
