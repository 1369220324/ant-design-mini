<view class="navigation-bar"/>

<ant-container title="基础用法">
  <prompts 
    list="{{ baseList }}" 
    promptsTitle="{{ promptsTitle }}"
    onClickPromptsItem="onClickPromptsItem"  />
</ant-container>

<ant-container title="有箭头">
  <prompts 
    list="{{ arrowList }}" 
    promptsTitle="{{ promptsTitle }}"
    onClickPromptsItem="onClickPromptsItem"  />
</ant-container>

<ant-container title="样式覆盖">
  <prompts 
    list="{{ styleList }}" 
    className="customizeStyle"
    promptsTitle="{{ promptsTitle }}"
    onClickPromptsItem="onClickPromptsItem"  />
</ant-container>
