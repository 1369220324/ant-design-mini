<ant-tabs
  items="{{items}}"
  current="{{current}}"
  onChange="onChange"
  direction="vertical"
  class="tabs"
  tabs-bar-class-name="tabs-bar"
  tab-active-class-name="tab-active"
>
  <scroll-view
    id="scroll-view"
    onScroll="onScroll"
    onTouchStart="onTouchStart"
    scroll-top="{{scrollTop}}"
    scroll-y="{{true}}"
    scroll-with-animation="{{true}}"
    scroll-animation-duration="{{300}}"
    class="content"
  >
    <block
      a:for="{{items}}"
      a:for-index="index"
      a:for-item="item"
    >
      <view class="item">
        <view
          class="title"
          id="tab-item-{{index}}"
        >
          {{item.title}}
        </view>
        <view>{{item.content}}</view>
      </view>
    </block>
  </scroll-view>
</ant-tabs>
<view class="footer"></view>