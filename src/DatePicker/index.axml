<import-sjs from="./index.sjs" name="_sjs" />
<picker 
    className="amd-date-picker"
    data="{{data}}"
    value="{{_sjs.getPickerValue(data,currentValue||cValue)}}"
    controlled
    disabled="{{disabled}}"
    title="{{title}}" 
    placeholder="{{placeholder}}"
    okText="{{okText}}"
    dismissText="{{dismissText}}"
    maskStyle="{{maskStyle}}"
    maskClass="{{maskClass}}"
    indicatorStyle="{{indicatorStyle}}"
    indicatorClass="{{indicatorClass}}"
    onChange="onChange"
    onDismiss="onDismiss"
    onOk="onOk"
    onTriggerPicker="onTriggerPicker"
    onFormat="onFormat">
    <slot name="title" slot="title"/>
</picker>