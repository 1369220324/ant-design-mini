<list 
  className="amd-radio-group {{className ? className : ''}}"
  radius="{{radius}}">
  <view class="amd-list-header">
    <slot name="header">
      {{header}}
    </slot>
  </view>
  <radio-group id="{{id ? id : $id}}"
    name="{{name ? name : ''}}"
    disabled="{{_disabled}}">
    <view class="amd-list-body">
      <slot />
    </view>
  </radio-group>
  <view class="amd-list-footer">
    <slot name="footer">
      {{footer}}
    </slot>
  </view>
</list>
