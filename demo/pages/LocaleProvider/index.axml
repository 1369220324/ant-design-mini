<ant-locale-provider >
  <ant-list>
    <ant-list-item>
      选择城市
      <ant-picker
        slot="extra"
        onCancel="handleDismissPicker"
        onVisibleChange="handleTriggerPicker"
        onOk="handleOk"
        defaultValue="{{ defaultValue }}"
        placeholder="请选择"
        title="请选择"
        onChange="handleChange"
        options="{{ list }}"
        emptyText="暂无数据"
      />
    </ant-list-item>
  </ant-list>
</ant-locale-provider>